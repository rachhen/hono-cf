{
  "name": "hono-cf",
  "version": "0.0.1",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "typecheck": "tsc",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
