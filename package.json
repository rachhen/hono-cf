    "drizzle-zod": "^0.7.0",
    "hono": "^4.7.2",
    "hono-openapi": "^0.4.5",
    "hono-pino": "^0.7.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "stoker": "^1.4.2",
    "zod": "^3.24.2",
    "zod-openapi": "^4.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@cloudflare/vitest-pool-workers": "^0.7.4",
    "@cloudflare/workers-types": "^4.20250224.0",
    "@libsql/client": "^0.14.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.21.0",
